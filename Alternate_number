import java .util.Scanner;

class Count_AlternateNumber
{
  public static void main(String[]a)
  {
   alt();

  }

  public static void alt()
    {
    System.out.println("Enter any number for alternation");
    Scanner sc=new Scanner (System.in);
    int num =sc.nextInt();
    
      while(num>0)
           {
            int rem=num%100;
            int a=rem/10;
    
            num=num/100;
         System.out.print( a+" ");
            }
     }
 }
