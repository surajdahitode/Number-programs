import java.util.Scanner;
public class Reverse_number {
	
	public static void main(String[]args){
		  
		rev();
		}
		public static void  rev(){
		int rev=0;
	
        
		int num=521956;
		  
		 while(num>0)
		       {

		        int rem=num%10;
		        rev=rev*10 +rem;
		        num= num/10;

		       }
		  System.out.println(rev);


		   
		   while (rev>0)
		       {
		        int rem=rev%10;
		   System.out.println(rem);
		   rev=rev/10;
		        }
		}
		}


